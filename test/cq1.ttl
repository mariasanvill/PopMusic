@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix tc: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/test/>  .
@prefix to: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/Ontology/popMusic.owl> .
@prefix pm: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/Ontology/> .
@prefix xds: <http://www.w3.org/2001/XMLSchema#>

tc:cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which is the album a certain song?" ;
 	owlunit:hasSPARQLUnitTest "SELECT ?Album {?SongVersion pm:hasTitle "Here comes the sun" ;
						  ?Album pm:contains ?SongVersion }" ;
	owlunit:hasInputData tc:datacq1.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"Album\" ] } ,  \"results\": {  \"bindings\": [ {  \"Album\": {  \"type\":  \"uri\" ,  \"value\":  \"https://github.com/mariasanvill/PopMusic/main/test/Abbey-Road\" } } ] } }";
	owlunit:testsOntology to: .
