@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix tc: <https://github.com/mariasanvill/PopMusic/blob/main/test/>  .
@prefix to: <https://github.com/mariasanvill/PopMusic/blob/main/Ontology/popMusic.owl> .
@prefix pm: <https://github.com/mariasanvill/PopMusic/blob/main/Ontology/> .

tc:cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which is the album a certain song?" ;
 	owlunit:hasSPARQLUnitTest "PREFIX pm: <https://github.com/mariasanvill/PopMusic>  SELECT ?Album {?SongVersion :hasTitle "Here comes the sun" .?Album :contains ?SongVersion }" ;
	owlunit:hasInputData tc:datacq1.ttl ;
	owlunit:hasInputTestDataCategory tc:ToyDataset.ttl ;
	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"Album\" ] } ,  \"results\": {  \"bindings\": [ {  \"Album\": {  \"type\":  \"uri\" ,  \"value\":  \"https://github.com/mariasanvill/PopMusic/toy-dataset/Abbey-Road\" } } ] } }";
	owlunit:testsOntology pm: .
