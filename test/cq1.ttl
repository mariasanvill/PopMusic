@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix tc: <https://github.com/mariasanvill/PopMusic/blob/main/test/>  .
@prefix td: <https://github.com/mariasanvill/PopMusic/main/toy-dataset/>.
@prefix to: <https://github.com/mariasanvill/PopMusic/main/Ontology/popMusic.owl> .
@prefix pm: <https://github.com/mariasanvill/PopMusic/main/Ontology/> .


tc:cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which is the album a certain song?" ;
 	owlunit:hasSPARQLUnitTest "SELECT ?Album {?Album pm:contains HereComesTheSun }" ;
	owlunit:hasInputData td:datacq1.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"Album\" ] } ,  \"results\": {  \"bindings\": [ {  \"Album\": {  \"type\":  \"uri\" ,  \"value\":  \"https://github.com/mariasanvill/PopMusic/main/toy-dataset/Abbey-Road\" } } ] } }";
	owlunit:testsOntology to: .
