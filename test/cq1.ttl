@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <https://github.com/mariasanvill/PopMusic/main/test/>  .
@prefix pm: <https://github.com/mariasanvill/PopMusic/main/Ontology/> .

ex:cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which is the album a certain song?" ;
 	owlunit:hasSPARQLUnitTest "PREFIX pm: <https://github.com/mariasanvill/PopMusic>  SELECT ?Album {?SongVersion :hasTitle "Here comes the sun" .?Album :contains ?SongVersion }" ;
	owlunit:hasInputData ex:datacq1.ttl ;
	owlunit:hasInputTestDataCategory test:ToyDataset ;
	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"interest\" ] } ,  \"results\": {  \"bindings\": [ {  \"interest\": {  \"type\":  \"uri\" ,  \"value\":  \"https://github.com/mariasanvill/PopMusic/Abbey Road\" } } ] } }";
	owlunit:testsOntology pm: .
