@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix tc: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/test/> .
@prefix td: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/toy-dataset/> .
@prefix pm: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/Ontology/pM.owl#> .
@prefix to: <https://raw.githubusercontent.com/mariasanvill/PopMusic/main/Ontology/pM.owl> .

tc:cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which is the album a certain song?" ;
 	owlunit:hasSPARQLUnitTest "prefix pm:<http://www.semanticweb.org/ke/ont/popMusic#> select ?Album where {?Album pm:containsSongVersions ?SongVersion}" ;
	owlunit:hasInputData td:datacq1.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"Album\" ] } ,  \"results\": {  \"bindings\": [ {  \"Album\": {  \"type\":  \"uri\" ,  \"value\": \"https://raw.githubusercontent.com/mariasanvill/PopMusic/main/Ontology/pM.owl#AbbeyRoad" } } ] } }";
	owlunit:testsOntology to: .
